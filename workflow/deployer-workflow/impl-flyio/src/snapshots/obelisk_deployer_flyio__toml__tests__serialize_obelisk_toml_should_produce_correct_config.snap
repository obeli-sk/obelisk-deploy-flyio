name = "stargazers_webhook"

[webhook_endpoint.location]
oci = "docker.io/getobelisk/demo_stargazers_webhook:2025-09-28@sha256:aa4dfa18d1ad7c1623163eeabb41a415ebad5296fca8f3b957987afcdb2a0f40"

[[webhook_endpoint.routes]]
methods = [
    "POST",
    "GET",
]
route = ""

[webui]
listening_addr = "[::]:8080"

[[workflow]]
name = "stargazers_workflow"

[workflow.location]
