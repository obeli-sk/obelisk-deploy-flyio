[[activity_wasm]]
env_vars = ["GITHUB_TOKEN"]
name = "stargazers_activity_github_impl"

[activity_wasm.exec.lock_expiry]
seconds = 5

[activity_wasm.location]
oci = "docker.io/getobelisk/demo_stargazers_activity_github_impl:2025-09-28@sha256:8f6fc9b1379b359e085998fa2fd7c966c450327d09770807dfba4b2f75731d72"

[[activity_wasm]]
env_vars = [
    "TURSO_TOKEN",
    "TURSO_LOCATION",
]
